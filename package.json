{
    "name": "cluecards",
    "private": true,
    "version": "0.0.0",
    "type": "module",
    "scripts": {
        "dev": "vite",
        "build": "vite build",
        "preview": "vite preview",
        "lint": "prettier --check . && eslint .",
        "format": "prettier --write . && eslint --fix .",
        "prettier": "prettier",
        "check": "svelte-check --tsconfig ./tsconfig.json && tsc -p tsconfig.node.json",
        "prepare": "npm run smui-theme-light && npm run smui-theme-dark",
        "smui-theme-light": "smui-theme compile smui/smui.css -i src/theme",
        "smui-theme-dark": "smui-theme compile smui/smui-dark.css -i src/theme/dark"
    },
    "devDependencies": {
        "@material/theme": "^14.0.0",
        "@mdi/js": "^7.4.47",
        "@smui-extra/accordion": "^7.0.0",
        "@smui/button": "^7.0.0",
        "@smui/card": "^7.0.0",
        "@smui/data-table": "^7.0.0",
        "@smui/icon-button": "^7.0.0",
        "@smui/layout-grid": "^7.0.0",
        "@smui/paper": "^7.0.0",
        "@smui/select": "^7.0.0",
        "@smui/textfield": "^7.0.0",
        "@smui/tooltip": "^7.0.0",
        "@sveltejs/vite-plugin-svelte": "^3.1.1",
        "@tsconfig/svelte": "^5.0.4",
        "@types/eslint": "^8.56.10",
        "eslint": "^8.57.0",
        "eslint-config-prettier": "^9.1.0",
        "eslint-plugin-svelte": "^2.42.0",
        "globals": "^15.8.0",
        "prettier": "^3.3.2",
        "prettier-plugin-svelte": "^3.2.5",
        "smui-theme": "^7.0.0",
        "svelte": "^4.2.18",
        "svelte-check": "^3.8.4",
        "tslib": "^2.6.3",
        "typescript": "^5.5.3",
        "typescript-eslint": "^7.16.0",
        "vite": "^5.3.3"
    },
    "dependencies": {
        "svelte-material-ui": "^7.0.0"
    }
}
